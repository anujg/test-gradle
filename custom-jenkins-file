#!groovy

pipeline {
    agent {label 'docker&&ubuntu1604&&language:java'}
    stages{
        stage('Build project') {
            steps {
                script {
                    withCredentials([string(credentialsId: 'SECRET1', variable: 'SECRET1')]) {
                    withCredentials([string(credentialsId: 'SECRET2', variable: 'SECRET2')]) {
                        sh './buildscripts/build.sh'
                    }
                    }
                }
            }
        }
    }
}